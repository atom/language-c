'.source.c, .source.cpp, .source.objc, .source.objcpp':
  'editor':
    'commentStart': '// '
    'increaseIndentPattern': '(?x)
       [\\{\\(]
      |^ \\s* (public|private|protected): \\s* $
      |^ \\s* @(public|private|protected) \\s* $
      |^ \\s* \\{ \\} $
      '
    'decreaseThisIndentPattern': '(?x)
       ^ \\s* (\\s* /[*] .* [*]/ \\s*)* \\}
      |^ \\s* (\\s* /[*] .* [*]/ \\s*)* \\)
      |^ \\s* (public|private|protected): \\s* $
      |^ \\s* @(public|private|protected) \\s* $
      '
    'decreaseIndentPattern': '(?x)
       [\\}\\)]
      '
'.source.c, .source.cpp':
  'editor':
    'foldEndPattern': '(?<!\\*)\\*\\*/|^\\s*\\}'
